<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>
<h1> Hello, Rails!</h1>

<h1 style="color:#FF0000">Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.title %>
    </li>
    <li>
      <%= article.body %>
    </li>
    <li>
      Hello World
    </li>
    <li>  <a href="/articles/<%= article.id %>">
      <%= article.title %>
    </a> </li>
    <li>
      <a href="<%= article_path(article) %>">
        <%= article.title %>
      </a>
    </li>




  <% end %>
</ul>